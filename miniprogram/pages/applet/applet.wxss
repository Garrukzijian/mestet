.content {
    width: 95%;
    margin-left: 3%;
    padding-top: 20rpx
}
.content1{
    padding-top: 60rpx
}

.head {
    width: 100%;
    height: 88rpx;
    font-size: 38rpx;
    font-weight: bold;
}
.head1{
    width: 100%;
    height: 88rpx;
    font-size: 38rpx;
    font-weight: bold;
}

.headerPart {
    width: 100;
    display: flex;
    justify-content: space-between;
    margin-top: 90rpx
}

.headerPart .imageBox {
    width: 31%;
    display: flex;
    flex-wrap: wrap;
    height: auto;
    position: relative;
    border-radius: 20rpx;
    padding-top: 70rpx;
    padding-bottom: 20rpx
}

.headerPart .backgrondFirst {
    background: linear-gradient(to bottom, rgb(246, 252, 252), rgb(245, 250, 253));
}

.headerPart .backgrondSecond {
    background: linear-gradient(to bottom, rgb(244, 252, 255), rgb(231, 249, 253));
}

.headerPart .backgrondThird {
    background: linear-gradient(to bottom, rgb(250, 249, 254), rgb(244, 244, 252));
}

.headerPart .imageBox .imageLogo {
    width: 170rpx;
    position: absolute;
    top: -60rpx;
    left: 10%
}

.headerPart .imageBox .imageTitle {
    width: 100%;
    font-size: 16px;
    font-weight: bold;
    text-align: center;
}

.headerPart .imageBox .useNumber {
    width: 100%;
    margin: 10rpx 0 20rpx 0;
    text-align: center;
    font-size: 10px;
    color: rgb(135, 136, 136);
}

.headerPart .imageBox .btnOpen {
    width: 60%;
    text-align: center;
    padding: 10rpx 0;
    font-weight: bold;
    margin-left: 20%;
    color: rgb(53, 159, 235);
    background-color: white;
    border-radius: 200rpx;
    overflow: hidden;
    font-size: 14px;
    border: none;
    position: relative
}
